# Environment Configuration
# Copy this file to .env.local and update with your actual values

# API Configuration
VITE_API_URL=http://localhost:3001

# Supabase Configuration (for database and authentication)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Maps API Key (for location services)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Analytics Configuration
VITE_ENABLE_ANALYTICS=true
VITE_GA_TRACKING_ID=your_google_analytics_id

# Feature Flags
VITE_ENABLE_CHAT=true
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE_MODE=false

# Business Configuration
VITE_BUSINESS_NAME="Sunrise Screening"
VITE_BUSINESS_PHONE="(555) 123-4567"
VITE_BUSINESS_EMAIL="info@sunrisescreening.com"
VITE_SERVICE_AREA="St. Petersburg, FL"

# Development Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info